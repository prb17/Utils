{
    "MetaData": { 
        "test_name" : "Array Functions Test",
        "test_type" : "unit"
    },
    "Tests" : [
        {
            "name" : "testArrayFind",
            "array" : [ 3, 6, 9, 12, 15, -11 ],
            "find" : 6,
            "expected" : 1
        },
        {
            "name" : "testArrayFind",
            "array" : [ 3, 6, 9, 12, 15, 11 ],
            "find" : 7,
            "expected" : -1
        },
        {
            "name" : "testArrayFind",
            "array" : [ true, false, true, false, true, false ],
            "find" : true,
            "expected" : 0
        },
        {
            "name" : "testArrayFind",
            "array" : [ 3.2, 6.3, 9.7, 12.5, 15.4, 11.1 ],
            "find" : 11.1,
            "expected" : 5
        },
        {
            "name" : "testArrayFind",
            "array" : [ 3.2, 6.3, 9.7, 12.5, 15.4, 11.1 ],
            "find" : 9.7,
            "expected" : 2
        }, 
        {
            "name" : "testArrayFind",
            "array" : [ "a", "b", "c", "d", "e" ],
            "find" : "d",
            "expected" : 3
        }, 
        {
            "name" : "testArrayFind",
            "array" : [ "one", "two", "three", "four", "five" ],
            "find" : "three",
            "expected" : 2
        },


