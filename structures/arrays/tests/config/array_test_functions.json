{
    "MetaData": 
    { 
        "test_name" : "Array Functions Test",
        "test_type" : "unit"
    },
    "Tests" : 
    {
        "testArrayFind": [
            {
                "array" : [ 3, 6, 9, 12, 15, -11 ],
                "find" : 6,
                "expected" : 1
            }, 
            {
                "array" : [ 3, 6, 9, 12, 15, 11 ],
                "find" : 7,
                "expected" : -1
            },
            {
                "array" : [ true, false, true, false, true, false ],
                "find" : true,
                "expected" : 0
            },
            {
                "array" : [ 3.2, 6.3, 9.7, 12.5, 15.4, 11.1 ],
                "find" : 11.1,
                "expected" : 5
            },
            {
                "array" : [ 3.2, 6.3, 9.7, 12.5, 15.4, 11.1 ],
                "find" : 9.7,
                "expected" : 2
            }, 
            {
                "array" : [ "a", "b", "c", "d", "e" ],
                "find" : "d",
                "expected" : 3
            }, 
            {
                "array" : [ "one", "two", "three", "four", "five" ],
                "find" : "three",
                "expected" : 2
            }
        ],
        "testArrayAdd" : [
            {
                "array" : [ 3, 6, 9 ],
                "add" : 6,
                "expected" : 4
            },
            {
                "array" : [ 3, 6, 9, 12, 15, 11 ],
                "add" : 7,
                "expected" : 6
            },
            {
                "array" : [ true, false, true, false, true, false ],
                "add" : true,
                "expected" : 6
            },
            {
                "array" : [ 3.2, 6.3, 9.7, 12.5, 15.4, 11.1 ],
                "add" : 11.1,
                "expected" : 6
            },
            {
                "array" : [ 3.2, 6.3, 9.7, 12.5, 15.4, 11.1 ],
                "add" : 9.7,
                "expected" : 6
            }, 
            {
                "array" : [ "a", "b", "c", "d", "e" ],
                "add" : "d",
                "expected" : 6
            }, 
            {
                "array" : [ "one", "two", "three", "four", "five" ],
                "add" : "three",
                "expected" : 6
            }
        ],
        "testArrayInsert" : [
            {
                "array" : [ 3, 6, 9 ],
                "insert" : 6,
                "idx" : 2,
                "expected" : 1
            },
            {
                "array" : [ 3, 6, 9, 12, 15, 11 ],
                "insert" : 7,
                "idx" : 2,
                "expected" : 2
            },
            {
                "array" : [ true, false, true, false, true, false ],
                "insert" : true,
                "idx" : 2,
                "expected" : 0
            },
            {
                "array" : [ 3.2, 6.3, 9.7, 12.5, 15.4, 11.1 ],
                "insert" : 11.1,
                "idx" : 2,
                "expected" : 2
            },
            {
                "array" : [ 3.2, 6.3, 9.7, 12.5, 15.4, 11.1 ],
                "insert" : 9.7,
                "idx" : 2,
                "expected" : 2
            }, 
            {
                "array" : [ "a", "b", "c", "d", "e" ],
                "insert" : "d",
                "idx" : 2,
                "expected" : 2
            }, 
            {
                "array" : [ "one", "two", "three", "four", "five" ],
                "insert" : "three",
                "idx" : 2,
                "expected" : 2
            }
        ]        
    }
}