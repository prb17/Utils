            },
            {
                "container1" : 0,
                "container2" : 1,
                "expected" : true
            },
            {
                "container1" : "1",
                "container2" : "0",
                "expected" : false
            },
            {
                "container1" : "0",
                "container2" : "1",
                "expected" : true
            }
        ],
        "testContainerGreaterThanEqual" : [
            {
                "container1" : 0,
                "container2" : 0,
                "expected" : true
            },
            {
                "container1" : "0",
                "container2" : "0",
                "expected" : true
            },
            {
                "container1" : 1,
                "container2" : 0,
                "expected" : true
            },
            {
                "container1" : 0,
                "container2" : 1,
                "expected" : false
            },
            {
                "container1" : "1",
                "container2" : "0",
